# OpenAI API Key
# Replace with your actual OpenAI API key - the current test key won't work
OPENAI_API_KEY=sk-test123456789
OPENAI_MODEL=gpt-4

# Ollama Configuration (Optional, for local models)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=ollama/deepseek-r1:8b

# AWS Bedrock Configuration (Optional, for AWS Bedrock)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_DEFAULT_REGION=us-west-2
BEDROCK_MODEL=deepseek.r1-v1:0

# LLM Configuration
# Try changing to "ollama" if you have Ollama running locally and don't have OpenAI API key
LLM_PROVIDER=bedrock  # Options: openai, ollama, bedrock
TEMPERATURE=0.2

# Debugging settings
LOG_LEVEL=INFO 